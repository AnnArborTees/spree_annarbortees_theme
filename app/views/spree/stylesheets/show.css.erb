@media (max-width: 767px) {
    .<%= @stylesheet.style_class %> #variantTabs .tab-content {
        background: <%= @stylesheet.product_options_background_color %>;
    }
    .<%= @stylesheet.style_class %>  .nav-tabs > li > a {
        background: <%= @stylesheet.product_options_option_background_color %>;
    }

    .<%= @stylesheet.style_class %>  .nav-tabs > li.active > a,
    .<%= @stylesheet.style_class %>  .nav-tabs > li.active > a:hover,
    .<%= @stylesheet.style_class %>  .nav-tabs > li.active > a:focus {
        background: <%= @stylesheet.product_options_background_color %>;
    }
}

@media (min-width: 768px) {
    .<%= @stylesheet.style_class %> #product-images-and-options {
        background: url('<%= @stylesheet.banner.url %>') no-repeat center center;
        background-size: cover;
        text-align: center;
    }
    .<%= @stylesheet.style_class %> #variantTabs .tab-content {
        background-color: <%= @stylesheet.product_options_background_color %>;
    }
    .<%= @stylesheet.style_class %>  .nav-tabs > li > a {
        background-color: <%= @stylesheet.product_options_option_background_color %>;
    }
    .<%= @stylesheet.style_class %>  .nav-tabs > li.active > a,
    .<%= @stylesheet.style_class %>  .nav-tabs > li.active > a:hover,
    .<%= @stylesheet.style_class %>  .nav-tabs > li.active > a:focus {
        background-color: <%= @stylesheet.product_options_background_color %>;
    }
}

